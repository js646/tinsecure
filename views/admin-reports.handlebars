<h1>Admin-Panel</h1>
<a href="/admin">
<button class="btn btn-outline-secondary" type="submit" id="submit-report">User List</button>
</a>
<a href="/admin/reports">
<button class="btn btn-outline-secondary" type="submit" id="submit-report">User Reports</button>
</a>

<div class="container">
    <div class="row col">
        <table class="table">
            <thead>
            <tr>
                <th scope="col">submitter</th>
                <th scope="col">reported</th>
                <th scope="col">message</th>
            </tr>
            </thead>
            <tbody>
            {{#each reports}}
            <tr>
                <th scope="row">{{{this.submitter}}}</th>
                <td>{{{this.reported}}}</td>
                <td>{{{this.message}}}</td>
            </tr>
            {{/each}}
            </tbody>
        </table>
    </div>
</div>